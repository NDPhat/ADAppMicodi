<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"

    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="99dp">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="420dp"
            android:text="Confirm Password"
            android:textColor="#3F51B5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/repassAdCreate"
            android:layout_width="331dp"
            android:layout_height="44dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="452dp"
            android:background="#FC66FFBD"
            android:drawableStart="@drawable/ic_24"
            android:drawableRight="@drawable/ic_showhint"
            android:drawablePadding="10dp"
            android:ems="10"
            android:hint="Re-Password"
            android:inputType="textPersonName"
            android:textColor="#FF227A"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="224dp"
            android:text="Email"
            android:textColor="#3F51B5"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="320dp"
            android:text="Password"
            android:textColor="#3F51B5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/passAdCreate"
            android:layout_width="331dp"
            android:layout_height="44dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="356dp"
            android:background="#FC66FFBD"
            android:drawableStart="@drawable/ic_24"
            android:drawableRight="@drawable/ic_showhint"
            android:drawablePadding="10dp"
            android:ems="10"
            android:hint="Password"
            android:inputType="textPersonName"
            android:textColor="#FF227A"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/emailAdCreate"
            android:layout_width="331dp"
            android:layout_height="44dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="260dp"
            android:background="#FC66FFBD"
            android:drawableStart="@drawable/ic_email"
            android:drawablePadding="10dp"
            android:ems="10"
            android:hint="abc@gmail.com"
            android:inputType="textPersonName"
            android:textColor="#FF227A"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btnAdCreate"
            android:layout_width="123dp"
            android:layout_height="38dp"
            android:layout_marginStart="156dp"
            android:layout_marginTop="632dp"
            android:text="Create"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Spinner
            android:id="@+id/AAUspinnerRole"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:layout_marginTop="16dp"
            android:background="@android:drawable/btn_dropdown"
            android:drawableStart="@drawable/ic_phone"
            android:spinnerMode="dropdown"
            android:textColor="#673AB7"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/repassAdCreate"></Spinner>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="148dp"
            android:layout_marginTop="116dp"
            android:fontFamily="monospace"
            android:text="Sign up"
            android:textColor="#3F51B5"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
