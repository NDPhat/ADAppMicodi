<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.ChitietActivity"
    android:id="@+id/chitietactivity">


    <EditText
        android:id="@+id/thanhphoCT"
        android:layout_width="243dp"
        android:layout_height="56dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="384dp"
        android:background="#FC66FFBD"
        android:drawableStart="@drawable/ic_city"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Thành phố"
        android:textColor="#FF227A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/duongCT"
        android:layout_width="243dp"
        android:layout_height="56dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="304dp"
        android:background="#FC66FFBD"
        android:drawableStart="@drawable/ic_distric"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Đường"
        android:textColor="#FF227A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/sonhaCT"
        android:layout_width="243dp"
        android:layout_height="56dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="224dp"
        android:background="#FC66FFBD"
        android:drawableStart="@drawable/ic_home"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Số nhà"
        android:textColor="#FF227A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/dienthoaiCT"
        android:layout_width="243dp"
        android:layout_height="56dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="152dp"
        android:background="#FC66FFBD"
        android:drawableStart="@drawable/ic_phone"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Điện thoại "

        android:textColor="#FF227A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/tenCT"
        android:layout_width="243dp"
        android:layout_height="56dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="80dp"
        android:background="#FC66FFBD"
        android:drawableStart="@drawable/ic_person"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Tên"
        android:textColor="#FF227A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnsubmitCT"
        android:layout_width="123dp"
        android:layout_height="38dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="468dp"
        android:text="Submit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="24dp"
        android:text="Thông tin cá nhân"
        android:textColor="#F44336"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>